#!/usr/bin/make -f
# -*- makefile -*-

#export DH_VERBOSE=1
export DH_OPTIONS

export PYBUILD_NAME=colorzero

%:
	dh $@ --with python2,python3,sphinxdoc --buildsystem=pybuild

#override_dh_auto_test:
